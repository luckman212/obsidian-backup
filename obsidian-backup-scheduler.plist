<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>obsidian-backup-scheduler</string>
	<key>Program</key>
	<string>/usr/local/bin/obsidian-backup.sh</string>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/tmp/obsidian-backup-scheduler.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/obsidian-backup-scheduler.out</string>
	<key>StartInterval</key>
	<integer>3600</integer>
</dict>
</plist>
